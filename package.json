{
  "name": "competentnl-sparql-agent",
  "version": "1.2.0",
  "description": "NL-naar-SPARQL agent met RAG ondersteuning",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start-backend": "node server.js",
    "start-frontend": "vite",
    "start": "concurrently \"npm run start-backend\" \"npm run start-frontend\"",
    "dev": "npm start",
    "seed-db": "node --loader ts-node/esm database/seed-embeddings.ts",
    "db-setup": "mysql -u root -p < database/schema.sql",
    "cnl:migrate": "node --loader ts-node/esm scripts/setup-cnl-classification.ts --migrate",
    "cnl:embeddings": "node --loader ts-node/esm scripts/setup-cnl-classification.ts --embeddings",
    "cnl:test": "node --loader ts-node/esm scripts/setup-cnl-classification.ts --test",
    "cnl:setup": "node --loader ts-node/esm scripts/setup-cnl-classification.ts --all"
  },
  "dependencies": {
    "@google/genai": "^1.34.0",
    "@xenova/transformers": "^2.17.2",
    "axios": "^1.7.2",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.22.1",
    "lucide-react": "^0.562.0",
    "mammoth": "^1.8.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.16.0",
    "node-fetch": "^2.7.0",
    "pdf-parse": "^1.1.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-dropzone": "^14.2.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/multer": "^1.4.12",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  }
}
