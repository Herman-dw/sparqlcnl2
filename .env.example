# CompetentNL SPARQL Agent - Environment Configuration
# ====================================================
# Kopieer dit bestand naar .env.local en vul de waarden in.

# === SPARQL Endpoint ===
COMPETENTNL_ENDPOINT=https://sparql.competentnl.nl
COMPETENTNL_API_KEY=jouw_competentnl_api_key

# === Gemini AI ===
GEMINI_API_KEY=jouw_gemini_api_key

# === MariaDB RAG Database ===
MARIADB_HOST=localhost
MARIADB_PORT=3306
MARIADB_USER=root
MARIADB_PASSWORD=jouw_wachtwoord
MARIADB_DATABASE=competentnl_rag

# === Server ===
PORT=3001
RAG_BACKEND_URL=http://localhost:3001
